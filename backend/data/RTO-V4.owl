    <!-- RTO状态约束 -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/rto_optimization#hasStatus"/>
        <owl:allValuesFrom>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>pending</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>running</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>success</rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                                <rdf:first>failed</rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:allValuesFrom>
        <rdfs:subClassOf rdf:resource="http://example.org/rto_optimization#RTOOptimization"/>
    </owl:Restriction>

    <!-- 约束类型约束 -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/rto_optimization#hasConstraintType"/>
        <owl:allValuesFrom>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>hard</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>soft</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </owl:allValuesFrom>
        <rdfs:subClassOf rdf:resource="http://example.org/rto_optimization#Constraint"/>
    </owl:Restriction>

    <!-- 新增：RTO优化必须属于某个工艺 -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/rto_optimization#belongsToProcess"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
        <rdfs:subClassOf rdf:resource="http://example.org/rto_optimization#RTOOptimization"/>
    </owl:Restriction>

    <!-- 新增：过程变量必须关联到设备 -->
    <owl:Restriction>
        <owl:onProperty rdf:resource="http://example.org/rto_optimization#measuredFrom"/>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</owl:minCardinality>
        <rdfs:subClassOf rdf:resource="http://example.org/rto_optimization#ProcessVariable"/>
    </owl:Restriction>

</rdf:RDF>
